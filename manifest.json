{
    "manifest_version": 3,
    "name": "HomeTerm",
    "version": "0.1.1",
    "action": {
        "default_title": "Open HomeTerm"
    },
    "background": {
        "service_worker": "sw.js"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "128": "media/image.png"
    },
    "chrome_url_overrides": {
        "newtab": "home.html"
    },
    "commands": {
        "open-home": {
            "suggested_key": {
                "default": "Alt+H"
            },
            "description": "Open HomeTerm"
        }
    },
    "web_accessible_resources": [
        {
            "resources": [
                "wallpapers/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}